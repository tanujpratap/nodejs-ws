<html>
    <head>
        <title>
            <%= title %>
        </title>
    </head>
    <body>
        <h1>
            contactlist
        </h1>
        <div>
            <ul>
               <% for(let i of contact_list ){%>
                <li>
                <p><%=i.name%></p>
                <p><%=i.phone%></p>
                </li>
                <%}%>
            </ul>

        </div> 
        <form action="/createcontact" method="POST">
            <input type="text" name="my_name" placeholder="Enter the name" required>
            <input type="number" name="my_phone" placeholder="enter the number" required>
            <button type="submit">Add Contacts</button>
        </form>
    </body>
</html>